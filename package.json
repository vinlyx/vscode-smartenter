{
    "name": "smartenter",
    "displayName": "Smart Enter",
    "description": "Automatically jump out of specific with enter",
    "version": "0.1.0",
    "publisher": "VincentLi",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "keybindings":[
            {
                "command": "smartEnter",
                "key": "enter",
                "when": "editorTextFocus && !suggestWidgetVisible && !inSnippetMode && !editorHasMultipleSelections"
            },
            {
                "command": "forceEnter",
                "key": "ctrl+enter",
                "when": "editorTextFocus && !suggestWidgetVisible && !inSnippetMode && !editorHasMultipleSelections"
            }
        ],
        "configuration": {
            "title": "%ext.smartenter.title%",
            "properties": {
                "smartenter.chars": {
                    "type": "string",
					"default": ")]}>/|;:=-,'\"",
					"description": "%ext.smartenter.chars%"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
